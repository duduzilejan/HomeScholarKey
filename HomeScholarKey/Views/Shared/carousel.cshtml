@{
    ViewBag.Title = "carousel";
}

<meta name="viewport" content="width=device-width" />
<title>Our SlideShow</title>
<link href="~/lib/bootstrap/css/bootstrap.min.css" rel="stylesheet" />
<h2 style="background-color: yellow; color:blue; text-align: center; font-style: oblique">Image Carousel</h2>
<fieldset>
    <legend style="font-family: arial black; color: navy">High Scholar Key</legend>
    <div class="container">
        <div class="row">
            <div class="col-md-8 col-md-offset-2">
                <div id ="imageCarousel" class="carousel slide" data-interval="2000" data-ride="carousel" data-pause="hover" data-wrap="true">
                    <ol class="carousel-indicators">
                        <li data-target="#imageCarousel" data-slide-to="0" class="active"></li>
                        <li data-target="#imageCarousel" data-slide-to="1"></li>
                        <li data-target="#imageCarousel" data-slide-to="2"></li>
                        <li data-target="#imageCarousel" data-slide-to="3"></li>
                    </ol>
                    <div class="carousel-inner">
                        <div class="item active">
                            <img src="image1.jpg" class="img-responsive" />
                            <div class="carousel-caption">
                                <h3></h3>
                                <p style="font-weight: bold; color:yellow; font-size:small">Our Community</p>
                            </div>
                            <div></div>
                            <div class="item">
                                <img src="image2.jpg" class="img-responsive" />
                                <div class="carousel-caption">
                                <h3></h3>
                                <p style="font-weight: bold; color:yellow; font-size:small">Our Community 2</p>
                                </div>
                            </div>
                              <div class="item">
                                <img src="image3.jpg" class="img-responsive" />
                                <div class="carousel-caption">
                                <h3></h3>
                                <p style="font-weight: bold; color:yellow; font-size:small">Our Community 3</p>
                                </div>
                            </div>
                              <div class="item">
                                <img src="image4.jpg" class="img-responsive" />
                                <div class="carousel-caption">
                                <h3></h3>
                                <p style="font-weight: bold; color:yellow; font-size:small">Our Communit y 4</p>
                                </div>
                            </div>
                              <div class="item">
                                <img src="image5.jpg" class="img-responsive" />
                                <div class="carousel-caption">
                                <h3></h3>
                                <p style="font-weight: bold; color:yellow; font-size:small">Our Community 5</p>
                                </div>
                            </div>
                            </div>
                            <a href="#imageCarousel" class="carousel-control left" data-slide="prev">
                                <span class="glyphicon glyphicon-hand-left"></span>
                            </a>
                            <a href="#imageCarousel" class="carousel-control right" data-slide="next">
                                <span class="glyphicon glyphicon-hand-right"></span>
                            </a>
                </div>
            </div>
        </div>
        </div>
        </div>
</fieldset>
<script src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.3/jquery.min.js"></script>
<script src=~/lib/bootstrap/js/bootstrap.min.js></script>
